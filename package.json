{
  "name": "eLab",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Nigel Maynard",
  "description": "Webapp for running audio/visual psych experiments online.",
  "angular-cli": {},
  "engines": {
    "node": "8.4.0",
    "npm": "5.3.0"
  },
  "scripts": {
    "postinstall": "ng build --aot -prod && tsc -p server",
    "ng": "ng",
    "build": "ng build",
    "start": "node dist/server/app.js",
    "predev": "tsc -p server",
    "devwoserver": "concurrently \"mongod\" \"ng serve -pc proxy.conf.json --open\" \"tsc -w -p server\"",
    "devsecure": "concurrently \"mongod\" \"ng serve -pc proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "dev": "concurrently \"mongod\" \"ng serve -pc proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "prod": "concurrently \"mongod\" \"ng build -aot -prod && tsc -p server && node dist/server/app.js\"",
    "test": "ng test",
    "testbe": "tsc -p server && node_modules/.bin/mocha dist/server/test",
    "lint": "ng lint",
    "lintbe": "tslint server/**/**.ts{,x}",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/cli": "^6.1.2",
    "@angular/common": "^6.1.1",
    "@angular/compiler": "^6.1.1",
    "@angular/compiler-cli": "^6.1.1",
    "@angular/core": "^6.1.1",
    "@angular/forms": "^6.1.1",
    "@angular/http": "^6.1.1",
    "@angular/platform-browser": "^6.1.1",
    "@angular/platform-browser-dynamic": "^6.1.1",
    "@angular/router": "^6.1.1",
    "@ng-bootstrap/ng-bootstrap": "^2.2.2",
    "@types/bowser": "^1.0.30",
    "@types/d3-scale": "^1.0.10",
    "@types/d3-scale-chromatic": "^1.1.0",
    "@types/socket.io": "^1.4.31",
    "@types/socket.io-client": "^1.4.31",
    "@types/youtube": "0.0.31",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "bowser": "^1.7.3",
    "core-js": "^2.4.1",
    "d3-scale": "^1.0.6",
    "d3-scale-chromatic": "^1.1.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "font-awesome": "^4.7.0",
    "googleapis": "^22.2.0",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^8.0.1",
    "jwt-decode": "^2.2.0",
    "lamejs": "^1.2.0",
    "mongoose": "^4.12.3",
    "morgan": "^1.8.2",
    "ngx-youtube-player": "^6.0.0",
    "normalize.css": "^8.0.0",
    "promise-retry": "^1.1.1",
    "rxjs": "^6.2.2",
    "shortid": "^2.2.8",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "socket.io-stream": "^0.9.1",
    "tether": "1.4.0",
    "typescript": "~2.9.2",
    "websocket-stream": "^5.0.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/language-service": "^6.1.1",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.90",
    "chai": "^4.1.0",
    "chai-http": "^3.0.0",
    "concurrently": "^3.3.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "^3.4.2",
    "nodemon": "1.11.0",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2"
  }
}
